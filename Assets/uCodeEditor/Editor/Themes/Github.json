{
    "name": "Github",
    "type": "light",
    "colors": {
        "editor.background": "#ffffff",
		"editor.foreground": "#24292e",
		"editorLineNumber.foreground": "#ccc",
		"editorIndentGuide.background": "#eee",
		"editorBracketMatch.background": "#f1f8ff",
		"editorBracketMatch.border": "#c8e1ff",
		"editor.lineHighlightBackground": "#fffbdd",
		"focusBorder": "#fafbfc",
		"activityBar.background": "#24292e",
		"sideBar.background": "#fafbfc",
		"sideBar.foreground": "#586069",
		"sideBar.border": "#ddd",
		"sideBarTitle.foreground": "#24292e",
		"sideBarSectionHeader.background": "#fafbfc",
		"sideBarSectionHeader.foreground": "#24292e",
		"statusBar.foreground": "#24292e",
		"statusBar.background": "#fafbfc",
		"statusBar.border": "#ddd",
		"statusBar.noFolderForeground": "#24292e",
		"statusBar.noFolderBackground": "#fafbfc",
		"statusBar.debuggingForeground": "#24292e",
		"statusBar.debuggingBackground": "#fafbfc",
		"notification.background": "#54a3ff"
    },
    "rules": [
        { "token": "comment", "foreground": "6a737d"},
        { "token": "type.identifier", "foreground": "6f42c1"},
        { "token": "string", "foreground": "032f62" },
        { "token": "constant.numeric", "foreground": "005cc5" },
        { "token": "constant.language", "foreground": "005cc5" },
        { "token": "constant.character", "foreground": "005cc5" },
        { "token": "constant.other", "foreground": "005cc5" },
        { "token": "keyword", "foreground": "d73a49" },
        { "token": "storage", "foreground": "c792ea" },
        { "token": "storage.modifier", "foreground": "d73a49" },
        { "token": "storage.type", "foreground": "d73a49" },
        { "token": "entity.name.type", "foreground": "6f42c1"},
        { "token": "entity.name.class", "foreground": "6f42c1" },
        { "token": "entity.other.inherited-class", "foreground": "6f42c1"},
        { "token": "entity.name.function", "foreground": "005cc5" },
        { "token": "entity.name.tag", "foreground": "22863a" },
        { "token": "entity.other.attribute-name", "foreground": "6f42c1" },
        { "token": "variable", "foreground": "6f42c1" },
        { "token": "variable.parameter", "foreground": "6f42c1" },
        { "token": "variable.other.const", "foreground": "6f42c1" },
        { "token": "support.function", "foreground": "005cc5" },
        { "token": "support.constant", "foreground": "005cc5" },
        { "token": "support.type", "foreground": "d73a49" },
        { "token": "support.class", "foreground": "d73a49" }
    ]
}